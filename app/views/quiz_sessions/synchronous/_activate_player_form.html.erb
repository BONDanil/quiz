<%= turbo_frame_tag dom_id(sessions_player, :player) do %>
  <%= form_with model: sessions_player, url: activate_player_quiz_session_path(sessions_player.quiz_session), method: :put do |f| %>
    <%= f.hidden_field :id %>
    <%= f.submit 'Activate player', class: 'btn btn-success' %>
  <% end %>
<% end %>
